<%= render partial: "welcome/index" %>
<%= render partial: "search" %>
<h2>List of tweets</h2>
<% @tweets.each do |tweet| %>
    <p>
    <h4><%= current_user.name %></h4><h5>@<%= current_user.username %></h5>
    <%= tweet.created_at %>
    <%= tweet.content %>
    </p>
<% end %>
