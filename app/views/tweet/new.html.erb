<h2>Create a tweet</h2>

<%= form_with url: :user_tweet_index, method: :post, model: @tweet do |f| %>
    <div class="form-inputs">
        <div class="form-floating">
            <%= f.text_area :content %>
        </div>
    </div>
    <div class="form-actions">
        <%= f.button :tweet %>
    </div>
<% end %>